{
  "name": "fantasy-video-games",
  "version": "1.0.0",
  "description": "fantasy video game app",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "google-auth-library": "^6.0.5",
    "googleapis": "^55.0.0",
    "linebyline": "^1.3.0",
    "moment": "^2.27.0",
    "mongo": "^0.1.0",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.24",
    "react-awesome-modal": "^2.0.5",
    "react-router-dom": "^5.2.0",
    "react-bootstrap": "^1.2.2",
    "readline": "^1.3.0",
    "readlines": "^0.2.0",
    "request": "^2.88.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
